
# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET     /getColumns                 controllers.MainController.getColumns
GET     /getCorrelationMatrix       controllers.MainController.getCorrelationMatrix
GET     /getSample                  controllers.MainController.getSample(percentage: Double)
POST    /postPredict                controllers.MainController.postPredict(gold_per_min: Int, level: Int, leaver_status: Int, xp_per_min: Int, radiant_score: Int, gold_spent: Int, deaths: Int, denies: Int, hero_damage: Int, tower_damage: Int, last_hits: Int, hero_healing: Int, duration: Int)
GET     /getGroupAndCount           controllers.MainController.getGroupAndCount(attribute: String, partitions: Option[Int])
